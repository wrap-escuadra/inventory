{% extends 'base.html.twig' %}

{% block title %}SupplierStatus index{% endblock %}

{% block body %}
    <h1>SupplierStatus index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Status_desc</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for supplier_status in supplier_statuses %}
            <tr>
                <td>{{ supplier_status.id }}</td>
                <td>{{ supplier_status.statusDesc }}</td>
                <td>
                    <a href="{{ path('supplier_status_show', {'id': supplier_status.id}) }}">show</a>
                    <a href="{{ path('supplier_status_edit', {'id': supplier_status.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('supplier_status_new') }}">Create new</a>
{% endblock %}